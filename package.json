{
  "name": "rianarai",
  "version": "3.0.0-beta.2",
  "private": "true",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.7.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "patch-package": "^6.4.7"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ]
  },
  "scripts": {
    "desktop": "yarn workspace @rianarai/desktop",
    "client": "yarn workspace @rianarai/desktop-client",
    "mobile": "yarn workspace @rianarai/mobile",
    "postinstall": "patch-package && yarn postinstall:desktop",
    "postinstall:desktop": "yarn --cwd ./apps/desktop electron-builder install-app-deps"
  },
  "resolutions": {
    "babel-preset-expo": "^8.5.1",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  }
}