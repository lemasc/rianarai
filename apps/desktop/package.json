{
  "private": true,
  "name": "@rianarai/desktop",
  "description": "RianArai (Insider Program)",
  "version": "0.1.0",
  "author": "Lemasc Service",
  "main": "main/background.js",
  "scripts": {
    "dev": "nextron",
    "pack": "nextron build --dir",
    "build:static": "postcss ./static/main.css -o ./static/index.css --config ./static/postcss.config.js",
    "build": "nextron build --win --ia32",
    "release": "nextron build --win --ia32 --publish always",
    "postinstall": "cd ../../ && yarn postinstall",
    "lint": "yarn workspace rianarai lint"
  },
  "workspaces": {
    "nohoist": [
      "**/electron",
      "**/electron/**"
    ]
  },
  "dependencies": {
    "@googleapis/classroom": "^0.2.0",
    "@sentry/electron": "^2.5.4",
    "@sinclair/typebox": "^0.20.5",
    "dayjs": "^1.10.7",
    "electron-serve": "^1.1.0",
    "electron-store": "^8.0.1",
    "electron-updater": "^4.3.9",
    "eventemitter3": "^4.0.7",
    "fastify": "^3.22.1",
    "fastify-plugin": "^3.0.0",
    "fuse.js": "^6.4.6",
    "google-auth-library": "^7.10.1",
    "keytar": "^7.7.0",
    "tiny-typed-emitter": "^2.1.0"
  },
  "devDependencies": {
    "@sentry/webpack-plugin": "^1.18.4",
    "@types/node": "^16.11.6",
    "@types/node-forge": "^0.10.9",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/sharp": "^0.29.5",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "autoprefixer": "^10.4.0",
    "electron": "^15.3.0",
    "electron-builder": "^22.13.1",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "<8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "nextron": "^7.1.0",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.1",
    "postcss-minify": "^1.1.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "tsee": "^1.3.2",
    "typescript": "^4.4.4"
  }
}